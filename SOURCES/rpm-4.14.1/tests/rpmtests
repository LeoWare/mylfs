    (('a', '=', '1.2-1'),	('a', '<>', '1.2-1'),	0),

    (('a', '=', '1.2-1'),	('a', '=', '1.2-2'),	0),
    (('a', '=', '1.2-1'),	('a', '>=', '1.2-2'),	0),
    (('a', '=', '1.2-1'),	('a', '<=', '1.2-2'),	1),
    (('a', '=', '1.2-1'),	('a', '<', '1.2-2'),	1),
    (('a', '=', '1.2-1'),	('a', '>', '1.2-2'),	0),
    (('a', '=', '1.2-1'),	('a', '<>', '1.2-2'),	1),

    (('a', '=', '1.3-1'),	('a', '=', '1.2-2'),	0),
    (('a', '=', '1.3-1'),	('a', '>=', '1.2-2'),	1),
    (('a', '=', '1.3-1'),	('a', '<=', '1.2-2'),	0),
