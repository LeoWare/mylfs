# Run sepdbugcrcfix on the binaries, both should have correct CRC already.
${abs_top_builddir}/sepdebugcrcfix ./usr/lib/debug \
  ./usr/local/bin/hello ./usr/local/bin/hello2 | grep CRC32 | cut -f2 -d:

