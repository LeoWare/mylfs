    (('a', '=', '1.3-1'),	('a', '<>', '1.2-2'),	1),

    # Zero epoch is same as no epoch
    (('a', '=', '0:1.2'),	('a', '=', '1.2'),	1),
    (('a', '=', '0:1.2'),	('a', '>=', '1.2'),	1),
    (('a', '=', '0:1.2'),	('a', '<=', '1.2'),	1),
    (('a', '=', '0:1.2'),	('a', '<', '1.2'),	0),
