    (('a',),			('a',),			1),
    (('a',),			('a', '=', '1.2'),	1),
    (('a',),			('a', '>=', '1.2'),	1),
    (('a',),			('a', '<=', '1.2'),	1),
    (('a',),			('a', '<', '1.2'),	1),
    (('a',),			('a', '>', '1.2'),	1),
    (('a',),			('a', '<>', '1.2'),	1),

