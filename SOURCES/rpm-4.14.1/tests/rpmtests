	--define \"pkg two\" \\
	--define \"provs deptest-foo = 1.0\" \\
	  /data/SPECS/deptest.spec

runroot rpm -U /build/RPMS/noarch/deptest-one-1.0-1.noarch.rpm /build/RPMS/noarch/deptest-two-1.0-1.noarch.rpm
